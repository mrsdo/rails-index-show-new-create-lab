<h1>Show All coupons</h1>
<section id="page">
  <table class="table-striped">
    <thead>
    <tr>
      <th>ID. </th>
      <th>Coupon Code</th>
      <th>Store</th>
      <th>Created At</th>
      <th>Updated At</th>
      <th>Edit</th>
      <th>Delete</th>
    </tr>
    </thead>

    <tbody>
    <% @coupons.each do |coupon| %>
      <tr>
        <td><%= coupon.id.to_s %></td>
        <td><%= coupon.coupon_code %></td>
        <td><%= coupon.store %></td>
        <td><%= coupon.created_at.to_s %></td>
        <td><%= coupon.updated_at.to_s %></td>
        <td><a href='/coupons/<%= coupon.id%>/edit'>Edit</a></td>
        <td> <a href="<%= @delete %>/coupons/<%= coupon.id %> ">Delete</a></td>
      </tr>
    <% end %>
    </tbody>
  </table>
  <nav>
    <ol>
      <li><a href="/coupons">coupons Home</a></li>
      <li><a href="/coupons/new">New</a></li>

    </ol>
  </nav>
</section>